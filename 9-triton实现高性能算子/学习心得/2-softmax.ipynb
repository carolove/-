   "source": [
    "import torch\n",
    "\n",
    "def native_softmax(x):\n",
    "    x_max = x.max(dim=1)[0]\n",
    "\n",
    "    z = x - x_max[:,None]\n",
    "\n",
    "    e = torch.exp(z)\n",
    "    sum = e.sum(dim=1)\n",
    "\n",
    "    return e/sum[:,None]\n",
    "\n",
    "\n",
    "x = torch.Tensor([[2,9],[54,1]])\n",
    "print(native_softmax(x))"
   ]
  }
